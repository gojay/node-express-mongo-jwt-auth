{
  "name": "express-typescript-crud",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Dani Rachmat Triyadi",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && NODE_PATH=./build node ./build/index.js",
    "compile": "tsc --build",
    "compile:watch": "tsc --build --watch",
    "pre:dev": "cross-env NODE_ENV=development NODE_PATH=./src nodemon -e ts,js --exec ts-node -r ./src/index.ts",
    "dev": "concurrently --kill-others \"yarn compile:watch\" \"yarn pre:dev\"",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "lint": "npx eslint ./src",
    "build": "npx tsc"
  },
  "keywords": [],
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "4.17.1",
    "express-jwt": "^7.7.5",
    "express-jwt-authz": "^2.4.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "mongoose": "^6.5.3",
    "node-jose": "^2.1.1",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "tslog": "^3.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "14.14.2",
    "@types/node-jose": "^1.1.10",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.5",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "concurrently": "^7.3.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
